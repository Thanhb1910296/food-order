<div style="height: 800px;">
    <div class="modal d-block" tabindex="-1">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Notification</h5>
              <a href="<%= typeof returnURL != 'undefined' ? returnURL : '/administrator' %>"><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button></a>
            </div>
            <div class="modal-body">
              <p><%= message %></p>
            </div>
            <div class="modal-footer">
              <a href="<%= typeof returnURL != 'undefined' ? returnURL : '/administrator' %>"><button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button></a> 
            </div>
          </div>
        </div>
      </div>
</div>
